<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender class="Log5PHP_Appender_Console" name="console">
    <layout class="Log5PHP_Layout_Simple"/>
    <param name="target" value="STDOUT"/>
  </appender>
  <appender class="Log5PHP_Appender_File" name="file">
    <layout class="Log5PHP_Layout_TTCC"/>
    <param name="file" value="server.log"/>
  </appender>
  <root>
    <level value="INFO"/>
    <appender_ref ref="file"/>
    <appender_ref ref="console"/>
  </root>
</configuration>
